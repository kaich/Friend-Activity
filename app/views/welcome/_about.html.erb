<!-- About Section -->
<section id="about">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading"> <%= t :track %></h2>
                <h3 class="section-subheading text-muted"> <%= t :track_descript %></h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <ul class="timeline">
                  <% for @event in track_activity_event %>
                    <%= render @event %>
                  <% end %> 
                </ul>
            </div>
        </div>
    </div>
</section>
