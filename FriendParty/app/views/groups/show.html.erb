<div class="panel panel-default">
  <div class="panel-body"> <%= @group.intro %> </div>
  <div class="panel-footer"> <%= @group.created_at %></div>
</div>

<%= render @group.applications %>

<% if !user_in_group? @group %>
  <%= render 'applications/form' %>
<% end %>

<table class="members" summary="Group members">
  <%= render @users %>
</table>
<%= will_paginate @users %>
